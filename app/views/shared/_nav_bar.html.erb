<% desk_page_name = 'Desk' %>
<% class_names = ['Household', 'Contact', 'Address', 'Correspondence', 'Label', 'List'] - [current_page] %>

<p>
  <% unless current_page == desk_page_name %>
    <%= fa_icon desk_page_name.constantize.fa_icon_string %><%= link_to desk_page_name, root_path %>
  <% end %>

  <% class_names.each do |class_name| %>
    <%= fa_icon class_name.constantize.fa_icon_string %><%= link_to class_name.pluralize, send("#{class_name.downcase.pluralize}_path") %> 
  <% end %>
</p>